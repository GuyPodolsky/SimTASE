<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>rse</servlet-name>
        <servlet-class>rse.servlets.RSEServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>rse.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActiveUsersServlet</servlet-name>
        <servlet-class>rse.servlets.ActiveUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowStockServlet</servlet-name>
        <servlet-class>rse.servlets.ShowStockServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rse</servlet-name>
        <url-pattern>/rse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActiveUsersServlet</servlet-name>
        <url-pattern>/rse/servlets/ActiveUsersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowStockServlet</servlet-name>
        <url-pattern>/servlets/ShowStockServlet</url-pattern>
    </servlet-mapping>
</web-app>
