<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>rse</servlet-name>
        <servlet-class>rse.servlets.RSEServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>rse.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>activeUsers</servlet-name>
        <servlet-class>rse.servlets.ActiveUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>showStock</servlet-name>
        <servlet-class>rse.servlets.ShowStockServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowAllStocksServlet</servlet-name>
        <servlet-class>rse.servlets.ShowAllStocksServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadXMLServlet</servlet-name>
        <servlet-class>rse.servlets.LoadXMLServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNewStockServlet</servlet-name>
        <servlet-class>rse.servlets.AddNewStockServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateUserDetails</servlet-name>
        <servlet-class>rse.servlets.UserDetailsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TradeCommandsServlet</servlet-name>
        <servlet-class>rse.servlets.TradeCommandsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rse</servlet-name>
        <url-pattern>/rse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>activeUsers</servlet-name>
        <url-pattern>/activeUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>showStock</servlet-name>
        <url-pattern>/showStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowAllStocksServlet</servlet-name>
        <url-pattern>/ShowAllStocksServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadXMLServlet</servlet-name>
        <url-pattern>/LoadXMLServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNewStockServlet</servlet-name>
        <url-pattern>/AddNewStockServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateUserDetails</servlet-name>
        <url-pattern>/UpdateUserDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TradeCommandsServlet</servlet-name>
        <url-pattern>/TradeCommands</url-pattern>
    </servlet-mapping>

</web-app>
